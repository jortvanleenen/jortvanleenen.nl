<head>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>

    <title>
        {% if page.title == nil or page.title == empty %}
            {{ page.name | split: "." | first | capitalize }}
        {% else %}
            {{ page.title }}
        {% endif %}
        - {{ site.title }}
    </title>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    {% feed_meta %}

    <link rel="stylesheet"
          href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@300..800&family=Fira+Code:wght@650&display=swap">
    <link rel="icon"
          href="data:image/svg+xml,
              <svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 100 100%22>
                <text y=%221em%22 font-size=%2280%22>🌐</text>
              </svg>">
    <link rel="stylesheet" href="/assets/css/styles.css">

    {% seo title = false %}
</head>